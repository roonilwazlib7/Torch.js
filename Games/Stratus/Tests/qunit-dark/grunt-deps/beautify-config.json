{
  "html": {
    "braceStyle": "collapse",
    "indentChar": " ",
    "indentScripts": "keep",
    "indentSize": 2,
    "maxPreserveNewlines": 2,
    "preserveNewlines": true,
    "unformatted": [
      "a",
      "sub",
      "sup",
      "b",
      "i",
      "u"
    ],
    "wrapLineLength": 0
  },
  "css": {
    "indentChar": " ",
    "indentSize": 2
  },
  "js": {
    "braceStyle": "collapse",
    "breakChainedMethods": false,
    "e4x": false,
    "evalCode": false,
    "indentChar": " ",
    "indentLevel": 0,
    "indentSize": 2,
    "indentWithTabs": false,
    "jslintHappy": true,
    "keepArrayIndentation": false,
    "keepFunctionIndentation": false,
    "maxPreserveNewlines": 2,
    "preserveNewlines": true,
    "spaceBeforeConditional": true,
    "spaceInParen": true,
    "unescapeStrings": false,
    "wrapLineLength": 0
  }
}